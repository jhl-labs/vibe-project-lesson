  // ===================================
  // Python Patterns
  // ===================================
  "PY: Clean Architecture Entity": {
    "scope": "python",
    "prefix": "py-entity",
    "body": [
      "from dataclasses import dataclass, field",
      "from datetime import datetime",
      "from uuid import uuid4",
      "",
      "",
      "@dataclass",
      "class ${1:Entity}:",
      "    \"\"\"${2:Entity description}.\"\"\"",
      "",
      "    id: str = field(default_factory=lambda: str(uuid4()))",
      "    ${3:property}: ${4:str}",
      "    created_at: datetime = field(default_factory=datetime.now)",
      "    updated_at: datetime = field(default_factory=datetime.now)",
      "",
      "    @classmethod",
      "    def create(cls, ${3:property}: ${4:str}) -> \"${1:Entity}\":",
      "        \"\"\"Create a new ${1:Entity}.\"\"\"",
      "        return cls(${3:property}=${3:property})",
      "",
      "    $0"
    ],
    "description": "Python Entity with dataclass"
  },
  "PY: Repository Protocol": {
    "scope": "python",
    "prefix": "py-repository",
    "body": [
      "from typing import Protocol",
      "",
      "",
      "class ${1:Entity}Repository(Protocol):",
      "    \"\"\"${1:Entity} repository interface.\"\"\"",
      "",
      "    async def find_by_id(self, id: str) -> ${1:Entity} | None:",
      "        \"\"\"Find ${1:Entity} by ID.\"\"\"",
      "        ...",
      "",
      "    async def find_all(self) -> list[${1:Entity}]:",
      "        \"\"\"Find all ${1:Entity}s.\"\"\"",
      "        ...",
      "",
      "    async def save(self, entity: ${1:Entity}) -> ${1:Entity}:",
      "        \"\"\"Save ${1:Entity}.\"\"\"",
      "        ...",
      "",
      "    async def delete(self, id: str) -> None:",
      "        \"\"\"Delete ${1:Entity}.\"\"\"",
      "        ...",
      "",
      "    $0"
    ],
    "description": "Python Repository Protocol"
  },
