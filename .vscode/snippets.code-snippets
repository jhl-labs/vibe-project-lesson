  "TS: Clean Architecture Entity": {
    "scope": "typescript",
    "prefix": "entity",
    "body": [
      "export interface ${1:Entity}Props {",
      "  id: string;",
      "  ${2:property}: ${3:type};",
      "  createdAt: Date;",
      "  updatedAt: Date;",
      "}",
      "",
      "export class ${1:Entity} {",
      "  private constructor(private readonly props: ${1:Entity}Props) {}",
      "",
      "  static create(props: Omit<${1:Entity}Props, 'id' | 'createdAt' | 'updatedAt'>): ${1:Entity} {",
      "    return new ${1:Entity}({",
      "      ...props,",
      "      id: crypto.randomUUID(),",
      "      createdAt: new Date(),",
      "      updatedAt: new Date(),",
      "    });",
      "  }",
      "",
      "  get id(): string { return this.props.id; }",
      "  get ${2:property}(): ${3:type} { return this.props.${2:property}; }",
      "  get createdAt(): Date { return this.props.createdAt; }",
      "  get updatedAt(): Date { return this.props.updatedAt; }",
      "",
      "  $0",
      "}"
    ],
    "description": "Clean Architecture Entity"
  },
  "TS: Repository Interface": {
    "scope": "typescript",
    "prefix": "repository",
    "body": [
      "export interface ${1:Entity}Repository {",
      "  findById(id: string): Promise<${1:Entity} | null>;",
