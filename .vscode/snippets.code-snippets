      "",
      "",
      "class ${1:UseCaseName}:",
      "    \"\"\"${6:Use case description}.\"\"\"",
      "",
      "    def __init__(self, ${7:repository}: ${8:RepositoryType}) -> None:",
      "        self._${7:repository} = ${7:repository}",
      "",
      "    async def execute(self, input: ${1:UseCaseName}Input) -> ${1:UseCaseName}Output:",
      "        \"\"\"Execute the use case.\"\"\"",
      "        $0"
    ],
    "description": "Python Use Case Pattern"
